    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@typescript-eslint/parser": "^5.51.0",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
