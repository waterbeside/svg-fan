    "arc",
    "sector",
    "circle",
    "扇形",
    "弧形"
  ],
  "author": "Natron Kan",
  "repository": {
    "type": "git",
    "url": "git@github.com:waterbeside/svg-fan.git"
  },
  "license": "MIT",
