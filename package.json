  "description": "This is a tool that can draw fans, arcs, or split circles.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "demo": "cross-env NODE_ENV=development rollup -c ./demo/rollup.config.ts --configPlugin typescript  -w ",
